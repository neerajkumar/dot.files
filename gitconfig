[color]
	diff = auto
	status = auto
	branch = auto

[core]
	excludesfile = /Users/neerajkumar/.gitignore
	editor = vim
	autocrlf = input

[alias]
  ci = commit        
  co = checkout
  c = commit -m
  a = add
  di = diff
  dic = diff --cached
  pl = pull
  ps = push
  plre = pull --rebase
  st = status
  out = log origin..HEAD
  bra = branch -a
  br = branch -a
  lg = "log --decorate --graph --oneline"
  fix = commit --amend -C HEAD

